{
  "manifest_version": 2,
  "name": "Toggle Native Tab Bar",
  "version": "0.9.1",
  "description": "Shows or hides the native tab bar",
  "homepage_url": "https://github.com/irvinm/Toggle-Native-Tab-Bar",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-visible.png",
    "default_title": "Toggle Native Tab Bar"
  },
  "applications": {
    "gecko": {
      "id": "Toggle-Native-Tab_Bar@irvinm.addons.mozilla.org"
    }
  },
  "web_accessible_resources": [
    "*.png"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon-hidden.png",
    "48": "icon-hidden.png",
    "128": "icon-hidden.png"
  }
}